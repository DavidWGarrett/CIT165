package chapter2_1;

public class Chapter2_1
//Demonstrates modulus and printing
{
	public static void main(String args[])
	{
		int a = 99;
		int b = 8;
		int result;
		int c = 3.14;
		result = a % b;
		System.out.println("Divide " + a " by " + b);
		System.out.println("remainder is " + c);
		System.out.print("c is the value of PI:");
		System.out.println(c);
	}
}